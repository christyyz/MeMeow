<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cats">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" android:maxSdkVersion="29"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/cat_img_app_icon_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/cat_img_app_icon_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.DayNight.NoActionBar">
        <activity android:name=".memeFromImage"></activity>
        <activity android:name=".imageUpload" />
        <activity android:name=".calledFromMkFromTxt" />
        <activity android:name=".TextEnteringActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>